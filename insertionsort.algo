PROCEDURE cardsexample(A : ARRAY_OF INTEGER[]);
VAR
    i,j,value:INTEGER;
    
BEGIN
    n:=A.length;
    FOR i FROM 1 TO n-1 DO
    value:=A[i];
    j:= i - 1;
    WHILE ((j >= 0) AND(A[j]>value)) DO
        A[j+1]:=A[j];
        j:=j-1;
    END_WHILE
    A[j+1]:=value;
    END_FOR
END
ALGORITHM insertion
VAR
    tab:ARRAY_OF INTEGER;
BEGIN
    cardsexample(tab);
END